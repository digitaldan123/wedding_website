$("#id").val("<%= @guest.id %>")
$("#guest_attending_yes").removeProp('checked').prop('checked', <%= @guest.attending_yes? %>)
$("#guest_attending_no").removeProp('checked').prop('checked', <%= @guest.attending_no? %>)
$("#guest_day_before_yes").removeProp('checked').prop('checked', <%= @guest.day_before_yes? %>)
$("#guest_day_before_no").removeProp('checked').prop('checked', <%= @guest.day_before_no? %>)
$("#guest_day_after_yes").removeProp('checked').prop('checked', <%= @guest.day_after_yes? %>)
$("#guest_day_after_no").removeProp('checked').prop('checked', <%= @guest.day_after_no? %>)
$("#guest_name").val("<%= j(@guest.name) %>")
$("#guest_last_name").val("<%= j(@guest.last_name) %>")
$("#guest_email").val("<%= j(@guest.email) %>")
$("#guest_food").val("<%= @guest.food %>")
$("#guest_dietary_restrictions").val("<%= j(@guest.dietary_restrictions) %>")
$("#guest_street").val("<%= j(@guest.street) %>")
$("#guest_city").val("<%= j(@guest.city) %>")
$("#guest_state").val("<%= j(@guest.state) %>")
$("#guest_zip").val("<%= j(@guest.zip) %>")
